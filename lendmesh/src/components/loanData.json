{
    "vehicle_loans": {
      "filter_data": [
        {
          "name": "Zip Code",
          "key": "zip_code",
          "type": "input",
          "value": "12345"
        },
        {
          "name": "Credit Score",
          "key": "credit_score",
          "type": "select",
                  "value": [
            { "name": "780", "key": 780 },
            { "name": "750", "key": 750 },
            { "name": "720", "key": 720 },
            { "name": "680", "key": 680 },
            { "name": "620", "key": 620 }
          ]
        },
        {
          "name": "Sort By",
          "key": "sort_by",
          "type": "select",
          "value": [{ "name": "APR", "key": "apr" }]
        },
        {
          "name": "Sort Type",
          "key": "sort_type",
          "type": "select",
          "value": [{ "name": "Ascending", "key": "asc" }, { "name": "Descending", "key": "desc" }]
        }
      ],
      "column_mappings": [
        {
          "name": "Lending Partner",
          "key": "lender_id",
          "unit": ""
        },
        {
          "name": "APR",
          "keys": ["overall_min_apr", "overall_max_apr"],
          "delimiter": " - ",
          "unit": "%"
        },
        {
          "name": "Term",
          "keys": ["overall_min_terms", "overall_max_terms"],
          "delimiter": " - ",
          "unit": "Yrs"
        },
        {
          "name": "Link",
          "key": "url",
          "unit": ""
        }
      ]
    },
